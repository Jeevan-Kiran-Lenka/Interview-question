def areAdding(arr, k):
    n = len(arr)
    setone = set(arr)
    for i in range(0,n-1):
        if((k - arr[i]) in setone):
            return 1
    return 0

# Driver program to test the above function
arr = [10, 15, 3, 7]
k = 17
# arr = [0,-1, 2]
# k = 1

if areAdding(arr, k):
    print("Yes")
else:
    print("Nope")


